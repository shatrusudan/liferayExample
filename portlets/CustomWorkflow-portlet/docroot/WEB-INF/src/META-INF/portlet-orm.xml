<?xml version="1.0"?>

<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete/>
		<persistence-unit-defaults>
			<access>PROPERTY</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<mapped-superclass class="com.workflow.demo.model.impl.FeedbackModelImpl">
		<attributes>
			<id name="feedbackId" />
			<basic name="uuid">
				<column name="UUID_" />
			</basic>
			<basic name="feedbackDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="feedbackText" />
			<basic name="feedbackSubject" />
			<basic name="feedBackStatus" />
			<basic name="statusByUserId" />
			<basic name="statusDate">
				<temporal>TIMESTAMP</temporal>
			</basic>
			<basic name="companyId" />
			<basic name="groupId" />
			<basic name="userId" />
			<transient name="modelAttributes" />
			<transient name="primaryKey" />
			<transient name="primaryKeyObj" />
			<transient name="statusByUserUuid" />
			<transient name="userUuid" />
		</attributes>
	</mapped-superclass>
	<entity class="com.workflow.demo.model.impl.FeedbackImpl" name="Feedback">
		<table name="test_Feedback" />
		<attributes />
	</entity>
</entity-mappings>